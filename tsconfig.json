{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "isolatedModules": true,
    "declaration": true,
    "rootDir": "src",
    "outDir": "dist",
    "importsNotUsedAsValues": "error"
  },
  "include": ["src"]
}
