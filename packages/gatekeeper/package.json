{
  "name": "@itsmapleleaf/gatekeeper",
  "description": "a slash command and interaction framework for discord.js",
  "author": "itsmapleleaf",
  "version": "0.5.0",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "files": [
    "dist",
    "bin",
    "codemods"
  ],
  "repository": "https://github.com/itsMapleLeaf/gatekeeper",
  "homepage": "https://github.com/itsMapleLeaf/gatekeeper#readme",
  "bugs": "https://github.com/itsMapleLeaf/gatekeeper/issues",
  "keywords": [
    "discord",
    "discord.js",
    "slash",
    "command",
    "commands",
    "interaction",
    "message",
    "component",
    "framework",
    "bot",
    "react",
    "reactive",
    "declarative"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "trash dist",
    "build": "pnpm run clean && rollup -c",
    "release": "pnpm run build && pnpm test && cpy ../../README.md . && release-it",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "peerDependencies": {
    "discord.js": ">=13"
  },
  "dependencies": {
    "chalk": "^4",
    "execa": "5.1.1",
    "fast-glob": "3.2.7",
    "jscodeshift": "0.13.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@babel/preset-typescript": "7.15.0",
    "@rollup/plugin-replace": "3.0.0",
    "@types/jest": "27.0.2",
    "@types/jscodeshift": "0.11.2",
    "@types/node": "16.10.3",
    "cpy-cli": "3.1.1",
    "discord.js": "^13.2.0",
    "esbuild": "0.13.4",
    "esbuild-jest": "0.5.0",
    "jest": "27.2.4",
    "jest-mock-console": "1.2.3",
    "release-it": "14.11.6",
    "rollup": "2.58.0",
    "rollup-plugin-ts": "1.4.7",
    "trash-cli": "4.0.0",
    "typescript": "4.4.3"
  },
  "bin": {
    "gatekeeper-migrate": "./bin/migrate.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "esbuild-jest"
    },
    "verbose": true,
    "globals": {
      "__TEST__": true
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ]
  },
  "release-it": {
    "github": {
      "release": true,
      "web": true
    }
  }
}
