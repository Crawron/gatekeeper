{
  "private": true,
  "scripts": {
    "dev": "pnpm -C packages/playground run dev",
    "build": "pnpm run --parallel build",
    "release": "pnpm run docs && git add docs && git commit -m docs && pnpm -C packages/gatekeeper run release",
    "lint": "eslint --ext js,ts,tsx .",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc packages/gatekeeper/src/main.ts",
    "lint-fix": "npm run lint -- --fix",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@itsmapleleaf/configs": "1.0.0",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.25.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "prettier": "2.4.1",
    "typedoc": "0.22.3",
    "typescript": "4.4.3"
  },
  "prettier": "@itsmapleleaf/configs/prettier",
  "eslintConfig": {
    "extends": [
      "./node_modules/@itsmapleleaf/configs/eslint"
    ]
  }
}
